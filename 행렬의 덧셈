#include <string>
#include <vector>

using namespace std;

vector<vector<int>> solution(vector<vector<int>> arr1, vector<vector<int>> arr2) {
    vector<vector<int>> answer;
    int sr = arr1.size();
    int sc = arr1[0].size();
    int t=0;
    for(int i=0; i <sr; i++){
        vector<int> vec1;
        for(int j=0; j<sc; j++){
            t = arr1[i][j] + arr2[i][j];
            vec1.push_back(t);
        }
        answer.push_back(vec1);
    }
    return answer;
}
