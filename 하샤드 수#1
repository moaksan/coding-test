#include <string>
#include <vector>

using namespace std;

bool solution(int x) {
    bool answer = true;
    int a = x / 1000;
	int b= (x / 100) -(a*10) ;
	int c=(x / 10)- (b*10) - (a*100) ;
	int d= (x / 1)- (c*10)-(b*100) - (a*1000) ;
    int y = a+b+c+d;
    if(x % y !=0)
        answer = false;
    return answer;
}
