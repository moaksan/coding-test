#include <string>
#include <vector>
#include <iostream>

using namespace std;

long long solution(long long n) {
    long long answer = 0;
    long long m;
    m = min((long long)7071067,n);
    for(long long i=1; i<=m; i++){
        if(n == (i*i)){
            answer = (i+1)*(i+1);
            break;
        }
        else answer =-1;
    }
    return answer;
}
